{
  "name": "onepage-portfolio-starter",
  "version": "0.2.0",
  "private": true,
  "scripts": {
  "dev": "next dev",
  "dev:turbo": "NEXT_TELEMETRY_DISABLED=1 next dev --turbo",
  "dev:local": "node --max-old-space-size=4096 ./node_modules/.bin/next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "node -e \"const fs=require('fs');['.next','node_modules'].forEach(p=>{try{fs.rmSync(p,{recursive:true,force:true});console.log('removed',p)}catch(e){}})\""
  },
  "dependencies": {
    "next": "14.2.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "clsx": "2.1.1",
    "framer-motion": "11.3.17"
  },
  "devDependencies": {
    "autoprefixer": "10.4.19",
    "postcss": "8.4.38",
    "tailwindcss": "3.4.7",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.4"
  }
}
